# Database
EXTERNAL_DB_HOST=your-postgres-host  # e.g., localhost, 192.168.1.100, or service name in Coolify
SQL_BASE=hmdm
SQL_USER=hmdm
SQL_PASS=your_secure_password_here

# Admin
ADMIN_EMAIL=your-email@your-domain.com

# Domain configuration
BASE_DOMAIN=your-mdm-domain.com

# Protocol (https for production)
PROTOCOL=https

# Security
SHARED_SECRET=your_secure_secret_here

# Headwind MDM Configuration
HMDM_VARIANT=os
DOWNLOAD_CREDENTIALS=
HMDM_URL=https://h-mdm.com/files/hmdm-5.33.2-os.war
CLIENT_VERSION=6.18

# Force reconfigure on first run
FORCE_RECONFIGURE=true